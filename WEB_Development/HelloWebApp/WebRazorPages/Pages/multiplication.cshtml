@page "/multi"
<h1> Multiplication Page </h1>
<br />
<br />
<p>Multiplication of 5 </p>
<table>
  
    <thead>
        <tr>
            <th>Number</th>
            <th>Multiplication Result</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 1; i <= 10; i++)
        {
            <tr>
                <td>@i</td>
                <td>@(5 * i)</td>
            </tr>
        }
    </tbody>
</table>
