<h1> Students Name</h1>
@model int[] //model binding to return view on controller
@{
	String[] Names = { "Rajan Tamang", "Sagar Khatri", "Aaryan Maharjan", "Pooja Bhattarai", "Ipshika Bhattarai" };
	
	var evens = Model.Where(n => n % 2 == 0);
	var odd = Model.Where(n => n % 2 == 1);


}
<h2> All numbers</h2>
 <div>
	 @foreach( int num in Model)
	{
		<span> @num</span>
	}
</div>
<h2> Even</h2>
@foreach (int n in evens)
{
	<i class="display-6">@(n+" ") </i>
}
<h2> Odds</h2>
@foreach (int n in odd)
{
	<i class="display-6">@(n + " ") </i>
}


<h2>Dynamic</h2>
<ol>
	@foreach (string name in Names)
	{
		<li> @name</li>


	}
</ol>

<h2>Static</h2>

<ol>
	<li> Rajan Tamang</li>
